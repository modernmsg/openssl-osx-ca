<plist version="1.0">
	<dict>
		<key>Label</key>
		<string>org.ra66i.openssl-osx-ca</string>

		<key>ProgramArguments</key>
		<array>
			<string>{BINDIR}/openssl-osx-ca</string>
			{ARGS}
			<string>{BREW}</string>
